generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  userName String @unique
  password String
  firstName String
  lastName String
  stars    Int    @default(0)
}
